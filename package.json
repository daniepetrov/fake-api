{
  "name": "shapka-selina",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "create-lock": "npm i --package-lock-only",
    "dev": "next dev",
    "build": "next build && npm prune --production",
    "build-local": "next build",
    "start": "NODE_ENV=production node server.js",
    "type-check": "tsc --noEmit",
    "format": "prettier --write 'src/**/*.{js,ts,tsx}'",
    "create-index": "create-index ./src/components/ui --update --recursive --extensions ts tsx",
    "tsc": "tsc --noEmit",
    "lint": "tsc --noEmit && eslint 'src/*/**/*.{js,ts,tsx}' --quiet --fix",
    "eslint": "eslint 'src/*/**/*.{js,ts,tsx}' --quiet --fix",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "upg": "npx npm-check -u"
  },
  "dependencies": {
    "@apollo/client": "^3.0.0-beta.34",
    "@apollo/react-ssr": "^4.0.0-beta.1",
    "@quentin-sommer/react-useragent": "^3.1.0",
    "@shopandshow/lp-custom-server": "^1.0.0",
    "@shopandshow/lp-next-config": "^1.0.0",
    "@svgr/webpack": "^5.1.0",
    "@xstyled/styled-components": "^1.16.0",
    "@xstyled/system": "^1.16.0",
    "apicache": "^1.5.3",
    "graphql": "^14.6.0",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.15",
    "next": "^9.2.1",
    "next-connect": "^0.6.0",
    "paths.macro": "^2.0.2",
    "react": "^16.12.0",
    "react-anchor-link-smooth-scroll": "^1.0.12",
