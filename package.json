    "react-input-mask": "^3.0.0-alpha.0",
    "react-intersection-observer": "^8.25.2",
    "react-is": "^16.12.0",
    "react-player": "^1.15.2",
    "react-slick": "^0.25.2",
    "styled-components": "^5.0.1",
    "ua-parser-js": "^0.7.21",
    "use-http": "^0.2.7"
  },
  "devDependencies": {
    "@shopandshow/lp-babel-config": "^1.0.0",
    "@shopandshow/lp-eslint-config": "^1.0.0",
    "@shopandshow/lp-prettier-config": "^1.0.0",
    "@shopandshow/lp-tsconfig": "^1.0.0",
    "@storybook/addon-actions": "^5.3.12",
    "@storybook/addon-docs": "^5.3.12",
    "@storybook/addon-links": "^5.3.12",
    "@storybook/addon-notes": "^5.3.12",
    "@storybook/addon-storysource": "^5.3.12",
    "@storybook/addons": "^5.3.12",
    "@storybook/preset-create-react-app": "^1.5.2",
    "@storybook/react": "^5.3.12",
    "@types/jest": "^25.1.2",
    "@types/node": "^13.7.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/styled-components": "^4.4.2",
    "@types/xstyled__system": "^1.14.1",
    "babel-plugin-graphql-tag": "^2.5.0",
    "babel-plugin-macros": "^2.8.0",
    "babel-plugin-react-require": "^3.1.3",
    "babel-plugin-styled-components": "1.10.2",
    "create-index": "^2.5.0",
    "eslint": "^6.8.0",
    "husky": "^4.2.1",
    "lint-staged": ">=10",
    "prettier": "^1.19.1",
